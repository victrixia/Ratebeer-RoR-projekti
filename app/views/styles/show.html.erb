<p id="notice"><%= notice %></p>

<p>
  <strong>Name:</strong>
  <%= @style.name %>
</p>

<p>
  <strong>Description:</strong>
  <%= @style.description %>
</p>
<p>
  <strong>Beers in this style:</strong>
  <ul>
  <% @style.beers.each do |beer| %>
     <li> <%= link_to beer.name, beer %></li>
  <% end %>
  </ul>
</p>
<%= link_to 'Edit', edit_style_path(@style) %> |
<%= link_to 'Back', styles_path %>
